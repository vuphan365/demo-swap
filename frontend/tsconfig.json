{
  "compilerOptions": {
    "outDir": "./built",
    "allowJs": true,
    "strictNullChecks": false,
    "target": "es5",
    "jsx": "react-jsx",
    "baseUrl": "src",
    "resolveJsonModule": true,
    "esModuleInterop": true
  },
  "include": [
    "./src/**/*"
  ],
  "exclude": [
    "**/node_modules"
  ],
}